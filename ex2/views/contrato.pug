extends layout 

block content 
    .w3-card-4 
        header.w3-container.w3-teal 
            h1 
                span Contrato
            
        .w3-container
            ul.w3-ul.w3-card-4(style="width:50%")
                li
                    b ID:
                    span= contrato._id
                li
                    b Objeto do Contrato:
                    span= contrato.objectoContrato
                li
                    b Data de Celebração do Contrato:
                    span= contrato.dataCelebracaoContrato
                li
                    b Preço Contratual:
                    span= contrato.precoContratual
                li
                    b NIPC da Entidade:
                    span= contrato.NIPC_entidade_comunicante
                li
                    b Entidade:
                    span= contrato.entidade_comunicante
extends layout

block content
    .w3-card-4
        header.w3-container.w3-teal
            h1
                span Página de Entidade 
                span Total:#{tot}$

    
        .w3-container
            table.w3-table-all
                tr
                    th Tipo de Procedimento
                    th Objeto do Contrato 
                    th Data de Publicação 
                    th Data de Celebração
                each contrato in lContratos
                    tr
                        td= contrato.tipoprocedimento
                        td= contrato.objectoContrato 
                        td= contrato.dataPublicacao
                        td= contrato.dataCelebracaoContrato
            
        footer.w3-container.w3-blue
            h5
                span Generated by EngWeb2024 in #{data} - [
                    a(href="/") Voltar
                span ]


                    

        